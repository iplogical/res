<#macro getCalendar>
    <div id='calendar'></div>
    <div id="eventContent" title="Event Details" style="display:none;">
        Start: <span id="eventStartTime"></span><br>
        End: <span id="eventEndTime"></span><br>
        Phone: <span id="eventPhone"></span><br>
        Guests: <span id="eventGuests"></span><br>
        <p id="eventInfo"></p>
    </div>
</#macro>