<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<Label fx:id="root" onDragDetected="#onTableDragged" onMouseClicked="#onTableClicked" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <graphic>
        <VBox fx:id="vBox" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="80.0" minWidth="80.0" prefHeight="80.0" prefWidth="80.0" style="-fx-border-color: #000000; -fx-border-width: 2; -fx-border-radius: 5; -fx-background-color: #FFFFFF; -fx-background-radius: 5;">
           <children>
              <Label fx:id="name" alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="%Table.Name" wrapText="true" />
              <Label fx:id="number" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="%Table.Number">
                 <font>
                    <Font name="System Bold" size="30.0" />
                 </font>
               <VBox.margin>
                  <Insets bottom="-5.0" top="-5.0" />
               </VBox.margin>
              </Label>
              <HBox alignment="CENTER">
                 <children>
                    <Label fx:id="guests" text="%Table.Guests" />
                    <Label text=" / " />
                    <Label fx:id="capacity" text="%Table.Capacity" />
                  <Label fx:id="note">
                     <font>
                        <Font name="Segoe UI Symbol" size="16.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="5.0" />
                     </HBox.margin>
                  </Label>
                 </children>
              </HBox>
           </children>
        </VBox>
    </graphic>
</Label>
